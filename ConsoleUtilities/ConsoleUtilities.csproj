<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net5.0</TargetFramework>
		<AssemblyName>J4JSoftware.ConsoleUtilities</AssemblyName>
		<RootNamespace>J4JSoftware.ConsoleUtilities</RootNamespace>
		<Nullable>enable</Nullable>
		<Authors>Mark A. Olbert</Authors>
		<Company>Jump for Joy Software</Company>
		<Product>J4JSoftware Console Utilities Library</Product>
		<Description>utilities for console applications</Description>
		<Copyright>Â© Mark A. Olbert all rights reserved</Copyright>
		<PackageDescription>utilities for console applications</PackageDescription>
		<RepositoryUrl>https://github.com/markolbert/ProgrammingUtilities</RepositoryUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
		<PackageIcon>Diego nuspec.png</PackageIcon>
		<RepositoryType>git</RepositoryType>
		<Version>0.8.0.0</Version>
		<AssemblyVersion>0.8.0.0</AssemblyVersion>
		<PackageReleaseNotes>initial public release</PackageReleaseNotes>
		<Configurations>Debug;Release;NuGetRelease</Configurations>
	</PropertyGroup>

	<Choose>
		<When Condition=" '$(Configuration)' == 'NuGetRelease' ">
			<ItemGroup>
				<PackageReference Include="J4JSoftware.Logging" Version="3.0.0" />
				<PackageReference Include="J4JSoftware.Logging.Autofac" Version="3.0.0" />
			</ItemGroup>
		</When>
		<Otherwise>
			<ItemGroup>
				<ProjectReference Include="..\..\J4JLogging\AutoFacJ4JLogging\AutofacJ4JLogging.csproj" />
				<ProjectReference Include="..\..\J4JLogging\J4JLogging\J4JLogging.csproj" />
			</ItemGroup>
		</Otherwise>
	</Choose>

	<ItemGroup>
	  <Compile Remove="AutofacConsoleUtilitiesModule.cs" />
	  <Compile Remove="UpdaterAttribute.cs" />
	</ItemGroup>

	<ItemGroup>
		<None Include="E:\Pix\JumpForJoy\Diego nuspec.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="Alba.CsConsoleFormat-NoXaml" Version="1.0.0" />
	</ItemGroup>

</Project>
