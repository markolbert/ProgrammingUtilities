<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net5.0-windows</TargetFramework>
		<UseWPF>true</UseWPF>
		<AssemblyName>J4JSoftware.WPFUtilities</AssemblyName>
		<RootNamespace>J4JSoftware.WPFUtilities</RootNamespace>
		<Nullable>enable</Nullable>
		<Authors>Mark Olbert</Authors>
		<Company>Jump for Joy Software</Company>
		<Description>viewmodel support for WPF projects</Description>
		<Copyright>© Mark A. Olbert all rights reserved</Copyright>
		<PackageDescription>utilities for console applications</PackageDescription>
		<RepositoryUrl>https://github.com/markolbert/ProgrammingUtilities</RepositoryUrl>
		<PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageIcon>Diego nuspec.png</PackageIcon>
		<RepositoryType>git</RepositoryType>
		<Version>0.8.0.0</Version>
		<AssemblyVersion>0.8.0.0</AssemblyVersion>
		<PackageReleaseNotes>initial public release</PackageReleaseNotes>
	</PropertyGroup>

	<ItemGroup>
	  <Compile Remove="range-tick\rounders\**" />
	  <EmbeddedResource Remove="range-tick\rounders\**" />
	</ItemGroup>

	<ItemGroup>
	  <Compile Remove="AutoFacWPFUtilities.cs" />
	  <Compile Remove="range-tick\DateRangeFocus.cs" />
	  <Compile Remove="range-tick\DecimalRangeCalculator.cs" />
	  <Compile Remove="range-tick\DoubleRangeCalculator.cs" />
	  <Compile Remove="range-tick\EndPointNature.cs" />
	  <Compile Remove="range-tick\IDateRangeCalculator.cs" />
	  <Compile Remove="range-tick\IntegerRangeCalculator.cs" />
	  <Compile Remove="range-tick\IRangeCalculator.cs" />
	  <Compile Remove="range-tick\MonthNumberRangeCalculator.cs" />
	  <Compile Remove="range-tick\MonthRangeCalculator.cs" />
	  <Compile Remove="range-tick\RangeCalculator-deprecated.cs" />
	  <Compile Remove="range-tick\RangeCalculator-deprecated.static-protected.cs" />
	  <Compile Remove="range-tick\RangeCalculator.static-protected.cs" />
	  <Compile Remove="range-tick\RangeParameters-deprecated.cs" />
	</ItemGroup>

	<ItemGroup>
	  <Page Remove="CustomComboBox.xaml" />
	</ItemGroup>

	<ItemGroup>
		<None Include="..\..\Media\JumpForJoy\Diego nuspec.png">
		<Pack>True</Pack>
		<PackagePath></PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="Microsoft.Toolkit.Mvvm" Version="7.0.2" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\..\J4JLogging\J4JLogging\J4JLogging.csproj" />
		<ProjectReference Include="..\DependencyInjection\DependencyInjection.csproj" />
	</ItemGroup>

</Project>
